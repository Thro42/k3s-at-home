version: 2
wifis:
  renderer: networkd
  wlan0:
    dhcp4: false
    optional: true
    access-points:
      [ssid]:
        password: [ssid_pwd]
    addresses:
      - [ip_address]
    gateway4: [gateway]
    nameservers:
      search: [[local-domain]]
      addresses: [[nameservers]]